{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Launch pdfToJson",
      "program": "${workspaceFolder}/${input:programName}",
      "pythonPath": "${workspaceFolder}/venv/Scripts/python.exe"
    }
  ],
  "inputs": [
    {
      "type": "promptString",
      "id": "programName",
      "description": "Enter the name of the Python file to run (e.g., pdfToJson.py)"
    }
  ]
}